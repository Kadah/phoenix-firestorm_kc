			<scroll_list name="experience_log_list">
				<columns name="time" label="Heure"/>
				<columns name="event" label="Évènement"/>
				<columns name="experience_name" label="Expérience"/>
				<columns name="object_name" label="Objet"/>
			</scroll_list>
			<button name="btn_notify" label="Notifier"/>
